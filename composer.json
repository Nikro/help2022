{
    "name": "nikro/help2022",
    "description": "A quick-and-dirty Drupal 9 implementation to help people during Rus-Ukr war.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://help2022.com",
    "support": {
        "docs": "https://github.com/Nikro/help2022"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.1",
        "drupal/captcha": "^1.2",
        "drupal/chosen": "^3.0",
        "drupal/coffee": "^1.2",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/ctools": "^3.7",
        "drupal/date_popup": "^1.1",
        "drupal/dropzonejs": "^2.5",
        "drupal/embed": "^1.4",
        "drupal/entity": "^1.2",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_group": "^3.2",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "^1.0",
        "drupal/honeypot": "^2.0",
        "drupal/mailsystem": "^4.3",
        "drupal/module_filter": "^3.2",
        "drupal/pathauto": "^1.9",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.7",
        "drupal/smart_trim": "^1.3",
        "drupal/structure_sync": "^2.0",
        "drupal/symfony_mailer": "^1.0",
        "drupal/token": "^1.10",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "harvesthq/chosen": "^1.8",
        "vlucas/phpdotenv": "^5.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        }
    },
    "require-dev": {
        "drupal/console": "^1.9",
        "drupal/devel": "^4.1",
        "drupal/examples": "^3.0",
        "drush/drush": "^10.6",
        "vijaycs85/drupal-quality-checker": "^1.5"
    }
}
